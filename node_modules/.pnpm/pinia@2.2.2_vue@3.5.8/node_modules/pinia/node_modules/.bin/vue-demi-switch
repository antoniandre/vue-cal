#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anto/localhost/vue-cal/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.8/node_modules/vue-demi/bin/node_modules:/Users/anto/localhost/vue-cal/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.8/node_modules/vue-demi/node_modules:/Users/anto/localhost/vue-cal/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.8/node_modules:/Users/anto/localhost/vue-cal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anto/localhost/vue-cal/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.8/node_modules/vue-demi/bin/node_modules:/Users/anto/localhost/vue-cal/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.8/node_modules/vue-demi/node_modules:/Users/anto/localhost/vue-cal/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.8/node_modules:/Users/anto/localhost/vue-cal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vue-demi@0.14.10_vue@3.5.8/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
else
  exec node  "$basedir/../../../../../vue-demi@0.14.10_vue@3.5.8/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
fi
